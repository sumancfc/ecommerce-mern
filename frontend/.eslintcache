es());\r\n    loadProduct();\r\n  }, [dispatch]);\r\n\r\n  const loadProduct = () => {\r\n    getProduct(slug)\r\n      .then((prod) => {\r\n        setValues({ ...values, ...prod.data });\r\n        //load single product category subs\r\n        gCategorySubs(prod.data.category._id).then((res) => {\r\n          setSubOptions(res.data); // on first load, show default subs\r\n        });\r\n        let arr = [];\r\n        prod.data.subs.map((s) => {\r\n          arr.push(s._id);\r\n        });\r\n        console.log(\"ARR\", arr);\r\n        setArrayOfSubs((prev) => arr); // required for ant design select to work\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  console.log(values);\r\n\r\n  //handle create product submit\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault();\r\n\r\n    setLoading(true);\r\n\r\n    values.subs = arrayOfSubs;\r\n    values.category = selectedCategory ? selectedCategory : values.category;\r\n\r\n    updateProduct(slug, values, authtoken)\r\n      .then((res) => {\r\n        setLoading(false);\r\n        addToast(`Product is updated`, {\r\n          appearance: \"success\",\r\n          autoDismiss: true,\r\n        });\r\n        history.push(\"/admin/products\");\r\n      })\r\n      .catch((error) => {\r\n        console.log(error.response.data.err);\r\n        addToast(error.response.data.err, {\r\n          appearance: \"error\",\r\n          autoDismiss: true,\r\n        });\r\n      });\r\n  };\r\n\r\n  //handle change\r\n  const handleChange = (e) => {\r\n    setValues({ ...values, [e.target.name]: e.target.value });\r\n  };\r\n\r\n  //handle category change\r\n  const handleCatagoryChange = (e) => {\r\n    e.preventDefault();\r\n    console.log(\"CLICKED CATEGORY\", e.target.value);\r\n    setValues({ ...values, subs: [] });\r\n\r\n    setSelectedCategory(e.target.value);\r\n\r\n    gCategorySubs(e.target.value)\r\n      .then((res) => {\r\n        console.log(\"SUB OPTION