onsole.log(error.response.data.err);\r\n        addToast(error.response.data.err, {\r\n          appearance: \"error\",\r\n          autoDismiss: true,\r\n        });\r\n      });\r\n  };\r\n\r\n  //handle change\r\n  const handleChange = (e) => {\r\n    setValues({ ...values, [e.target.name]: e.target.value });\r\n  };\r\n\r\n  //handle category change\r\n  const handleCatagoryChange = (e) => {\r\n    e.preventDefault();\r\n    console.log(\"CLICKED CATEGORY\", e.target.value);\r\n    setValues({ ...values, subs: [], category: e.target.value });\r\n\r\n    gCategorySubs(e.target.value)\r\n      .then((res) => {\r\n        console.log(\"SUB OPTIONS ON CATGORY CLICK\", res.data);\r\n        setSubOptions(res.data);\r\n      })\r\n      .catch((err) => console.log(err));\r\n    setShowSub(true);\r\n  };\r\n\r\n  return (\r\n    <AdminProfile title='Create Products'>\r\n      {/* Image Upload Btn */}\r\n      <ImageUpload\r\n        setLoading={setLoading}\r\n        values={values}\r\n        setValues={setValues}\r\n      />\r\n\r\n      {/* Product Create Form */}\r\n      <ProductForm\r\n        categories={categories}\r\n        handleChange={handleChange}\r\n        handleSubmit={handleSubmit}\r\n        setValues={setValues}\r\n        values={values}\r\n        handleCatagoryChange={handleCatagoryChange}\r\n        subOptions={subOptions}\r\n        showSub={showSub}\r\n      />\r\n    </AdminProfile>\r\n  );\r\n};\r\n\r\nexport default CreateProduct;\r\n","C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\components\\button\\LinkButton.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\pages\\admin\\products\\AllProducts.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\components\\header\\Header.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\pages\\admin\\common\\ProductForm.js",["320","321","322","323","324"],"import React from \"