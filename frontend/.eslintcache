NS ON CATGORY CLICK\", res.data);\r\n        setSubOptions(res.data);\r\n      })\r\n      .catch((err) => console.log(err));\r\n    setShowSub(true);\r\n  };\r\n\r\n  return (\r\n    <AdminProfile title='Create Products'>\r\n      {/* Image Upload Btn */}\r\n      <ImageUpload\r\n        setLoading={setLoading}\r\n        values={values}\r\n        setValues={setValues}\r\n      />\r\n\r\n      {/* Product Create Form */}\r\n      <ProductForm\r\n        categories={categories}\r\n        handleChange={handleChange}\r\n        handleSubmit={handleSubmit}\r\n        setValues={setValues}\r\n        values={values}\r\n        handleCatagoryChange={handleCatagoryChange}\r\n        subOptions={subOptions}\r\n        showSub={showSub}\r\n      />\r\n    </AdminProfile>\r\n  );\r\n};\r\n\r\nexport default CreateProduct;\r\n","C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\components\\button\\LinkButton.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\pages\\admin\\products\\AllProducts.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\components\\header\\Header.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\pages\\admin\\common\\ProductForm.js",["338","339","340","341","342"],"import React from \"react\";\r\nimport Button from \"../../../components/button\";\r\nimport { Select } from \"antd\";\r\n\r\nconst { Option } = Select;\r\n\r\nconst ProductForm = ({\r\n  categories,\r\n  handleChange,\r\n  handleSubmit,\r\n  setValues,\r\n  values,\r\n  handleCatagoryChange,\r\n  subOptions,\r\n  showSub,\r\n}) => {\r\n  const {\r\n    title,\r\n    description,\r\n    price,\r\n    category,\r\n    subs,\r\n    shipping,\r\n    quantity,\r\n    images,\r\n    colors,\r\n    brands,\r\n    color,\r\n    brand,\r\n  } = values;\r\n\r\n  return (\r\n    <div className='mt-45'>\r\n      <form onSubmit={handleSubmit}>\r\n        <div className='input__item'>\r\n          <input\r\n            type='text'\r\n            name='title'\r\n            value={title}\r\n            placeholder='Enter product name'\r\n            onChange={handleChange}\r\n            required\r\n          />\r\n        </div>\r\n\r\n        <div className='input__item'>\r\n          <input\r\n            type='text'\r\n            name='description'\r\n            value={description}\r\n            placeholder='Enter description'\r\n            onChange={handleChange}\r\n            required\r\n          />\r\n        </div>\r\n\r\n        <div className='input__item'>\r\n          <input\r\n            type='number'\r\n            name='price'\r\n            value={price}\r\n            placeholder='Enter price'\r\n            onChange={handleChange}\r\n            required\r\n          />\r\n        </div>\r\n\r\n        <div className='input__item'>\r\n          <select name='shipping' onChange={handleChange}>\r\n            <option>Please select</option>\r\n            <option value='No'>No</option>\r\n            <option value='Yes'>Yes</option>\r\n          </select>\r\n        </div>\r\n\r\n        <div className='input__item'>\r\n          <input\r\n            type='number'\r\n            name='quantity'\r\n            value={quantity}\r\n            placeholder='Enter quantity'\r\n            onChange={handleChange}\r\n            required\r\n          />\r\n        </div>\r\n\r\n        <div className='input__item'>\r\n          <select name='color' onChange={handleChange}>\r\n            <option>Please select</option>\r\n            {colors.map((color) => (\r\n              <option key={color} value={color}>\r\n                {color}\r\n              </option>\r\n            ))}\r\n          </select>\r\n        </div>\r\n\r\n        <div className='input__item'>\r\n          <select name='brand' onChange={handleChange}>\r\n            <option>Please select</option>\r\n            {brands.map((brand) => (\r\n              <option key={brand} value={brand}>\r\n                {brand}\r\n              </option>\r\n            ))}\r\n          </select>\r\n        </div>\r\n\r\n        <div className='input__item'>\r\n          <select name='category' onChange={handleCatagoryChange}>\r\n            <option>Please select</option>\r\n            {categories.length > 0 &&\r\n              categories.map((c) => (\r\n                <option key={c._id} value={c._id}>\r\n                  {c.name}\r\n                </option>\r\n              ))}\r\n          </select>\r\n        </div>\r\n\r\n        {showSub && (\r\n          <div className='input__item'>\r\n            <Select\r\n              mode='multiple'\r\n              style={{ width: \"100%\" }}\r\n              placeholder='Please select'\r\n              value={subs}\r\n              onChange={(value) => setValues({ ...values, subs: value })}\r\n            >\r\n              {subOptions.length &&\r\n                subOptions.map((s) => (\r\n                  <Option key={s._id} value={s._id}>\r\n                    {s.name}\r\n                  </Option>\r\n                ))}\r\n            </Select>\r\n          </div>\r\n        )}\r\n\r\n        <Button title='Create' />\r\n      </form>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default ProductForm;\r\n","C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\helpers\\product.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\store\\constants\\index.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\pages\\admin\\common\\ImageUpload.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\pages\\About.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\pages\\auth\\Register.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Project\\ecommerce-mern\\frontend\\src\\pages\\auth\\RegisterComplete.js",[],"C:\\Users\\sumancfc\\Desktop\\React-Pr